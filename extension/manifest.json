{
  "name": "Better History",
  "version": "1.2.0",
  "description": "A better look at your browsing history. The best searching, the sharpest interface, and the most useful filters - for your history.",
  "default_locale":"en",
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "history": "index.html"
  },
  "browser_action": {
    "default_icon": "images/icons/icon19.png",
    "default_title": "Browse history"
  },
  "background_page": "background.html",
  "permissions": ["chrome://favicon/", "history", "tabs", "contextMenus"]
}
