{
  "name": "__MSG_manifest_name__",
  "version": "1.6.0",
  "manifest_version": 2,
  "description": "__MSG_manifest_description__",
  "default_locale":"en",
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  },
  "chrome_url_overrides": {
    "history": "index.html"
  },
  "browser_action": {
    "default_icon": "images/icons/icon19.png",
    "default_title": "__MSG_manifest_browser_action_title__"
  },
  "background": {
    "scripts": [
      "javascripts/frameworks/underscore-min.js",
      "javascripts/frameworks/backbone-min.js",
      "javascripts/namespace.js",
      "javascripts/lib/backbone_extensions.js",
      "javascripts/lib/context_menus/page_context_menu.js",
      "javascripts/lib/context_menus/selection_context_menu.js",
      "javascripts/lib/browser_actions.js",
      "javascripts/models/base.js",
      "javascripts/models/settings.js",
      "javascripts/helpers/url_builder.js",
      "javascripts/initialize_background.js"
    ]
  },
  "permissions": [
    "chrome://favicon/",
    "history",
    "tabs",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://platform.twitter.com https://apis.google.com; object-src 'self'"
}
