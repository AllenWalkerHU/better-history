{
  "extended_formal_date": {
    "message": "$weekday$, $day$ $month$ $year$",
    "description": "The order of a full extended date",
    "placeholders": {
      "weekday": {
        "content": "$1",
        "example": "March"
      },
      "month": {
        "content": "$2",
        "example": "March"
      },
      "day": {
        "content": "$3",
        "example": "25"
      },
      "year": {
        "content": "$4",
        "example": "2011"
      }
    }
  },
  "formal_date": {
    "message": "$day$ $month$ $year$",
    "description": "The order of a full date",
    "placeholders": {
      "month": {
        "content": "$1",
        "example": "March"
      },
      "day": {
        "content": "$2",
        "example": "25"
      },
      "year": {
        "content": "$3",
        "example": "2011"
      }
    }
  },

  "google_plus_language": {
    "message": "en-GB",
    "description": "Google plus language abbreviation"
  }
}
